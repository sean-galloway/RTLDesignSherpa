python3 $REPO_ROOT/bin/vcd2wavedrom2.py -i dump.vcd -c config_start.json -o wavedrom_wrr_start.json
python3 $REPO_ROOT/bin/vcd2wavedrom2.py -i dump.vcd -c config_end.json -o wavedrom_wrr_end.json
perl -pi -e 's/weighted_round_robin_wrapper.//' wavedrom_wrr_start.json
perl -pi -e 's/weighted_round_robin_wrapper.//' wavedrom_wrr_end.json
